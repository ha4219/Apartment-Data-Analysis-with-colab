from sys import stdin, maxsize


n, k = map(int, input().split())

a = list(map(int, input().split()))

d = [[maxsize] * (n+1) for _ in range(n+1)]

def f(l, r):
    if l==r:
        d[l][r] = 0
        return d[l][r]
    if d[l][r] != maxsize:
        return d[l][r]
    
    for i in range(l+1, r):
        w = 0
        if a[l]!=a[r]:
            w=1
        d[l][r] = min(d[l][r], d[l][l+i]+d[l+1][r] + w)
    return d[l][r]
    
print(f(0, n))
